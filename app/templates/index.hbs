<div class="row">
  <div class="col s8 animalsDiv">
    {{animal-tile model=model}}
  </div>

  <div class="col s4 usersDiv">
    <h4>Sign-up!</h4>
    {{new-user addUser="addUser"}}
    <br>
    <div class="divider">
    </div>
    <h4>users:</h4>
    <ul>
      {{#each model.users as |user|}}
        {{#link-to 'password' user.id user=model}}<li>{{user.name}}</li>{{/link-to}}
      {{/each}}
    </ul>

  </div>
</div>
